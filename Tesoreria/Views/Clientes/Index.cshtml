@model IEnumerable<Tesoreria.Models.Clientes>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Clientes", "Create", "Clientes")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Idenficacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombres)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellidos)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Idenficacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombres)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellidos)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDCliente }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDCliente }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IDCliente })
        </td>
    </tr>
}

</table>
